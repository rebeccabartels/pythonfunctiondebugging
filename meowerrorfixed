stage.set_background("houseinterior")
whiskers = codesters.Sprite("cat", -100, -100)
tiger = codesters.Sprite("kitten", 300, -150)
tiger.flip_right_left()

def whiskersfunc():
    tiger.glide_to(50, -150)
    stage.wait(2)
    whiskers.say("Meow.")
    stage.wait(2)
    whiskers.say("Meow. Meow.")
    stage.wait(2)
    whiskers.say("Meow.")
    tiger.flip_right_left()
    tiger.say("mew, mew")
    tiger.glide_to(300, -150)

whiskersfunc()
