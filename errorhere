sprite = codesters.Sprite("dinosaur", 0, -125)
sprite.set_speed(2)

def dino band():
    stage.set_background("concert")
    sprite.say("My band is called Rock-asaurous Rex")
    sprite.turn_left(360)


dino_band()



# RULE 1: Function names cannot have any spaces in them. Combine two word names with an underscore _ , like this: function_name.
